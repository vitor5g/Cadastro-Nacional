<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>indexToEndereco</from-outcome>
            <to-view-id>/Endereco.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>indexToPais</from-outcome>
            <to-view-id>/Pais.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>indexToCidade</from-outcome>
            <to-view-id>/Cidade.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id>/Endereco.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>enderecoToIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/Endereco.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>enderecoToForm</from-outcome>
            <to-view-id>/EnderecoForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <description>controlador da entidade endereco</description>
        <managed-bean-name>enderecoController</managed-bean-name>
        <managed-bean-class>frb.edu.br.vitor.controller.EnderecoController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/EnderecoForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>formToEndereco</from-outcome>
            <to-view-id>/Endereco.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/EnderecoForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Pais.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>paisToIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/Pais.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>paisToForm</from-outcome>
            <to-view-id>/PaisForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PaisForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>formToPais</from-outcome>
            <to-view-id>/Pais.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/PaisForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Cidade.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cidadeToIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/Cidade.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cidadeToForm</from-outcome>
            <to-view-id>/CidadeForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CidadeForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>formToCidade</from-outcome>
            <to-view-id>/Cidade.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/CidadeForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>paisController</managed-bean-name>
        <managed-bean-class>frb.edu.br.vitor.controller.PaisController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cidadeController</managed-bean-name>
        <managed-bean-class>frb.edu.br.vitor.controller.CidadeController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>indexController</managed-bean-name>
        <managed-bean-class>frb.edu.br.vitor.controller.IndexController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
